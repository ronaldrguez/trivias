rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    match /trivias/{entry} {
			allow read: if request.auth.uid != null;
      allow write:
        if request.auth.uid != null;
    }
    match /{document=**} {
      allow read, write: if request.auth != null;
    }
  }
}